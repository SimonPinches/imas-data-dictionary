<?modxslt-stylesheet type="text/xsl" media="screen" alternate="no" title="Show raw source of the XML file" charset="ISO-8859-1" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="../Utilities/DD_Support.xsd"/>
	<xs:complexType name="Equilibrium_Boundary">
		<xs:annotation>
			<xs:documentation>Geometry of the plasma boundary</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Type">
				<xs:annotation>
					<xs:documentation>0 (limiter) or 1 (diverted)</xs:documentation>
					<xs:appinfo>
						<axis1>../../Timebase</axis1>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="N_Outline_Points">
				<xs:annotation>
					<xs:documentation>Number of meaningful Outline points in R and Z at a given time.</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Outline_RZ">
				<xs:annotation>
					<xs:documentation>RZ description of the plasma boundary. The second dimension is oversized to fit the maximum number of (R,Z) points over the time range</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>1...2</axis1>
						<axis2>1...Max_N_Outline_Points</axis2>
						<axis3>../../Timebase</axis3>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_3D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Geometric_Axis_RZ">
				<xs:annotation>
					<xs:documentation>RZ position of the geometric axis (defined as (Rmin+Rmax) / 2 of the boundary)</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>1...2</axis1>
						<axis2>../../Timebase</axis2>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="a_minor">
				<xs:annotation>
					<xs:documentation>Minor radius of the plasma boundary (defined as (Rmax-Rmin) / 2 of the boundary)</xs:documentation>
					<xs:appinfo>
						<axis1>../../Timebase</axis1>
						<type>SIGNAL</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Elongation">
				<xs:annotation>
					<xs:documentation>Elongation of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<axis1>../../Timebase</axis1>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Elongation_Upper">
				<xs:annotation>
					<xs:documentation>Elongation (upper half w.r.t. geometric axis) of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<axis1>../../Timebase</axis1>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Elongation_Lower">
				<xs:annotation>
					<xs:documentation>Elongation (lower half w.r.t. geometric axis) of the plasma boundary</xs:documentation>
<xs:appinfo>
						<axis1>../../Timebase</axis1>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Triangularity_Upper">
				<xs:annotation>
					<xs:documentation>Upper triangularity of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<axis1>../../Timebase</axis1>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Triangularity_Lower">
				<xs:annotation>
					<xs:documentation>Lower triangularity of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<axis1>../../Timebase</axis1>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="N_X_Points">
				<xs:annotation>
					<xs:documentation>Number of X points at a given time</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="X_Points_RZ">
				<xs:annotation>
					<xs:documentation>RZ position of the X points, first is the active X point if diverted. The second dimension is oversized to fit the maximum number of X points over the time range</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>1...2</axis1>
						<axis2>1...Max_N_X_Points</axis2>
						<axis3>../../Timebase</axis3>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_3D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Strike_Point_Lower_Left_RZ">
				<xs:annotation>
					<xs:documentation>RZ position of the lower left strike point</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>1...2</axis1>
						<axis2>../../Timebase</axis2>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Strike_Point_Upper_Left_RZ">
				<xs:annotation>
					<xs:documentation>RZ position of the upper left strike point</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>1...2</axis1>
						<axis2>../../Timebase</axis2>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Strike_Point_Lower_Right_RZ">
				<xs:annotation>
					<xs:documentation>RZ position of the lower right strike point</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>1...2</axis1>
						<axis2>../../Timebase</axis2>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Strike_Point_Upper_Right_RZ">
				<xs:annotation>
					<xs:documentation>RZ position of the upper right strike point</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>1...2</axis1>
						<axis2>../../Timebase</axis2>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Active_Limiter_Point_RZ">
				<xs:annotation>
					<xs:documentation>RZ position of the active limiter point (point of the plasma boundary in contact with the limiter)</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>1...2</axis1>
						<axis2>../../Timebase</axis2>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Equlibrium_Global">
		<xs:annotation>
			<xs:documentation>0D parameters of the equilibrium</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Beta_Pol">
				<xs:annotation>
					<xs:documentation>Poloidal beta</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Beta_Tor">
				<xs:annotation>
					<xs:documentation>Toroidal beta</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Beta_Normal">
				<xs:annotation>
					<xs:documentation>Normalised toroidal beta</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Ip">
				<xs:annotation>
					<xs:documentation>Plasma current. Positive sign means anti-clockwise when viewed from above.</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="li_3">
				<xs:annotation>
					<xs:documentation>Internal inductance</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Volume">
				<xs:annotation>
					<xs:documentation>Total plasma volume</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
						<units>m^3</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Area">
				<xs:annotation>
					<xs:documentation>Area of the poloidal cross section</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
						<units>m^2</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Psi_Axis">
				<xs:annotation>
					<xs:documentation>Poloidal flux at the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Psi_Boundary">
				<xs:annotation>
					<xs:documentation>Poloidal flux at the selected plasma boundary</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Magnetic_Axis" type="RZ1D_SIGNAL">
				<xs:annotation>
					<xs:documentation>Magnetic axis position</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="B_Tor_Axis">
				<xs:annotation>
					<xs:documentation>Total toroidal magnetic field at the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="q_Axis">
				<xs:annotation>
					<xs:documentation>q at the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="q_95">
				<xs:annotation>
					<xs:documentation>q at the 95% poloidal flux surface</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="q_Min">
				<xs:annotation>
					<xs:documentation>Minimum q value in the plasma</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Vacuum_Toroidal_Field" type="B_Tor_Vacuum_2">
				<xs:annotation>
					<xs:documentation>Characteristics of the vacuum toroidal field</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="W_MHD">
				<xs:annotation>
					<xs:documentation>Plasma energy content = 3/2 * int(p,dV) with p being the total pressure (thermal + fast particles) [J]. Time-dependent; Scalar</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
						<units>J</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Gamma">
				<xs:annotation>
					<xs:documentation>Adiabatic index</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../../Timebase</axis1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Equilibrium_Profiles_1D">
		<xs:annotation>
			<xs:documentation>Equilibrium profiles (1D radial grid) as a function of the poloidal flux</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Psi">
				<xs:annotation>
					<xs:documentation>Poloidal flux, such that Bp=|grad psi| /R/2/pi</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>1...N_Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Phi">
				<xs:annotation>
					<xs:documentation>Toroidal flux</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Pressure">
				<xs:annotation>
					<xs:documentation>Pressure</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>Pa</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="F">
				<xs:annotation>
					<xs:documentation>Diamagnetic function (F=R B_Phi)</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>T.m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="dPressure_dPsi">
				<xs:annotation>
					<xs:documentation>Derivative of Pressure w.r.t. Psi</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>Pa.Wb^-1</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="F_dF_dPsi">
				<xs:annotation>
					<xs:documentation>Derivative of F w.r.t. Psi, multiplied with F</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>T^2.m^2/Wb</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="J_Tor">
				<xs:annotation>
					<xs:documentation>Flux surface averaged toroidal current density = average(J_TorR) / average(1/R)</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>A/m^2</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="J_Parallel">
				<xs:annotation>
					<xs:documentation>Flux surface averaged parallel current density = average(j.B) / B0, where B0 = Equilibrium/Global/Toroidal_Field/B0</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>A/m^2</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="q">
				<xs:annotation>
					<xs:documentation>Safety factor</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Magnetic_Shear">
				<xs:annotation>
					<xs:documentation>Magnetic shear</xs:documentation>
					<xs:appinfo>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>-</units>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="R_Inboard">
				<xs:annotation>
					<xs:documentation>Radial coordinate (major radius) on the inboard side of the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="R_Outboard">
				<xs:annotation>
					<xs:documentation>Radial coordinate (major radius) on the outboard side of the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Rho_Tor">
				<xs:annotation>
					<xs:documentation>Toroidal flux coordinate, defined as sqrt(Phi/pi/B0), where B0 = Equilibrium/Global/Toroidal_Field/B0</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<axis1>../Psi</axis1>
						<axis2>../../Timebase</axis2>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
