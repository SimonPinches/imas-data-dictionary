					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="frequencies">
				<xs:annotation>
					<xs:documentation>Array of frequencies scanned during a sweep</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>Hz</units>
						<coordinate1>1...N</coordinate1>
