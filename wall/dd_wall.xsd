					<xs:documentation>Quantities related to electrons</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="neutral" type="wall_global_quantitites_neutral" maxOccurs="5">
				<xs:annotation>
					<xs:documentation>Quantities related to the various neutral species</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="temperature">
				<xs:annotation>
					<xs:documentation>Wall temperature</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>K</units>
						<coordinate1>../../time</coordinate1>
