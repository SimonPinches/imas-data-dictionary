				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="dynamic">
				<xs:annotation>
					<xs:documentation>Absolute error provided as a dynamic of same dimension as the related field.</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="constant">
				<xs:annotation>
					<xs:documentation>Relative error in % provided as a constant</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="expression">
				<xs:annotation>
					<xs:documentation>Absolute error provided as an Expression.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="dynamic_type">
		<xs:annotation>
			<xs:documentation>Structure for 1D dynamic (prototype to be evaluated)</xs:documentation>
			<xs:appinfo>parent-dependent</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="time"/>
			<xs:element name="value">
				<xs:annotation>
					<xs:documentation>Value of the dynamic. Value/Data is ugly. Moreover the metadata are not at the right place. </xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<unit>Defined by its parent</unit>
						<coordinate1>../time</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:group ref="FLT_1D"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="b_tor_vacuum_1">
		<xs:annotation>
			<xs:documentation>Characteristics of the vacuum toroidal field. time assumed to be one level above</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="r0">
				<xs:annotation>
					<xs:documentation>Reference major radius where the vacuum toroidal magnetic field is given (usually a fixed position such as the middle of the vessel at the equatorial midplane)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="b0">
				<xs:annotation>
					<xs:documentation>Vacuum toroidal field at R0 [T]; Positive sign means anti-clockwise when viewed from above. The product R0B0 must be consistent with the b_tor_vacuum_r field of the tf IDS.</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>T</units>
						<coordinate1>../../time</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="b_tor_vacuum_2">
		<xs:annotation>
			<xs:documentation>Characteristics of the vacuum toroidal field. time assumed to be two levels above</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="r0">
				<xs:annotation>
					<xs:documentation>Reference major radius where the vacuum toroidal magnetic field is given (usually a fixed position such as the middle of the vessel at the equatorial midplane)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="b0">
				<xs:annotation>
					<xs:documentation>Vacuum toroidal field at b0; Positive sign means anti-clockwise when viewed from above. The product r0*b0 must be consistent with the b_tor_vacuum_r field of the tf IDS.</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>T</units>
						<coordinate1>../../../time</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
