						<xs:documentation>Code parameters schema</xs:documentation>
						<xs:appinfo>
							<type>constant</type>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="errorbar">
		<xs:annotation>
			<xs:documentation>Structure for 1D dynamic errorbar</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="type">
				<xs:annotation>
					<xs:documentation>Type of errorbar description which is used. Refers to the name of the sibling fields.</xs:documentation>
