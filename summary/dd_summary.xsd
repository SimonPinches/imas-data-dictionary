	<xs:complexType name="summary_sol">
		<xs:annotation>
			<xs:documentation>Scrape-Off-Layer characteristics</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="t_e_decay_length" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Electron temperature radial decay length inv(grad Te/Te)</xs:documentation>
					<xs:appinfo>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="t_i_average_decay_length" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Ion temperature (average over ion species) radial decay length inv(grad Ti/Ti)</xs:documentation>
					<xs:appinfo>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="n_e_decay_length" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Electron density radial decay length inv(grad ne/ne)</xs:documentation>
					<xs:appinfo>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="n_i_total_decay_length" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Ion density radial decay length inv(grad ni/ni)</xs:documentation>
					<xs:appinfo>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="heat_flux_e_decay_length" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Electron heat flux radial decay length inv(grad qe/qe)</xs:documentation>
					<xs:appinfo>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="heat_flux_i_decay_length" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Ion heat flux radial decay length inv(grad qi/qi)</xs:documentation>
					<xs:appinfo>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="power_radiated" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Power radiated from the SOL</xs:documentation>
					<xs:appinfo>
						<units>W</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pressure_neutral" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Neutral pressure in the SOL</xs:documentation>
					<xs:appinfo>
						<units>Pa</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="summary_average_quantities">
		<xs:annotation>
			<xs:documentation>Set of average quantities</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="t_e" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Electron temperature</xs:documentation>
					<xs:appinfo>
						<units>eV</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="t_i_average" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Ion temperature (average over ion species)</xs:documentation>
					<xs:appinfo>
						<units>eV</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="n_e" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Electron density</xs:documentation>
					<xs:appinfo>
						<units>m^-3</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="n_i" type="summary_species_level_2">
				<xs:annotation>
					<xs:documentation>Ion density per species</xs:documentation>
					<xs:appinfo>
						<units>m^-3</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="n_i_total" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Total ion density (sum over species)</xs:documentation>
					<xs:appinfo>
						<units>m^-3</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="zeff" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Effective charge</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="summary_global_quantities">
		<xs:annotation>
			<xs:documentation>Various global quantities calculated from the fields solved in the transport equations and from the Derived Profiles</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ip" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Total plasma current</xs:documentation>
					<xs:appinfo>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="current_non_inductive" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Total non-inductive parallel current</xs:documentation>
					<xs:appinfo>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="current_bootstrap" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Bootstrap parallel current</xs:documentation>
					<xs:appinfo>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="current_ohm" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Ohmic parallel current</xs:documentation>
					<xs:appinfo>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="current_alignment" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Figure of merit of the alignment of the current profile sources, defined in the following reference: http://iopscience.iop.org/article/10.1088/0029-5515/43/7/318</xs:documentation>
					<xs:appinfo>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="v_loop" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>LCFS loop voltage</xs:documentation>
					<xs:appinfo>
						<units>V</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="li" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Internal inductance. The li_3 definition is used, i.e. li_3 = 2/R0/mu0^2/Ip^2 * int(Bp^2 dV).</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="beta_tor" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Toroidal beta, defined as the volume-averaged total perpendicular pressure divided by (B0^2/(2*mu0)), i.e. beta_toroidal = 2 mu0 int(p dV) / V / B0^2</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="beta_tor_norm" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Normalised toroidal beta, defined as 100 * beta_tor * a[m] * B0 [T] / ip [MA] </xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="beta_tor_thermal_norm" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Normalised toroidal beta from thermal pressure only, defined as 100 * beta_tor_thermal * a[m] * B0 [T] / ip [MA] </xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="beta_pol" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Poloidal beta. Defined as betap = 4 int(p dV) / [R_0 * mu_0 * Ip^2]</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="energy_diamagnetic" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Plasma energy content = 3/2 * integral over the plasma volume of the total perpendicular pressure </xs:documentation>
					<xs:appinfo>
						<units>J</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="energy_total" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Plasma energy content = 3/2 * integral over the plasma volume of the total kinetic pressure</xs:documentation>
					<xs:appinfo>
						<units>J</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="energy_thermal" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Thermal plasma energy content = 3/2 * integral over the plasma volume of the thermal pressure</xs:documentation>
					<xs:appinfo>
						<units>J</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="energy_b_field_pol" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Poloidal magnetic plasma energy content = 1/(2.mu0) * integral over the plasma volume of b_field_pol^2</xs:documentation>
					<xs:appinfo>
						<units>J</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="volume" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Volume of the confined plasma</xs:documentation>
					<xs:appinfo>
						<units>m^3</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="h_mode" type="summary_dynamic_int_1d_1">
				<xs:annotation>
					<xs:documentation>H-mode flag: 0 when the plasma is in L-mode and 1 when in H-mode </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="r0" type="summary_constant_flt_0d">
				<xs:annotation>
					<xs:documentation>Reference major radius where the vacuum toroidal magnetic field is given (usually a fixed position such as the middle of the vessel at the equatorial midplane)</xs:documentation>
					<xs:appinfo>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="b0" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Vacuum toroidal field at R0. Positive sign means anti-clockwise when viewed from above. The product R0B0 must be consistent with the b_tor_vacuum_r field of the tf IDS.</xs:documentation>
					<xs:appinfo>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="h_98" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Energy confinement time enhancement factor over the IPB98(y,2) scaling</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="tau_energy" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Energy confinement time</xs:documentation>
					<xs:appinfo>
						<units>s</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="tau_helium" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Helium confinement time</xs:documentation>
					<xs:appinfo>
						<units>s</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="tau_resistive" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Current diffusion characteristic time</xs:documentation>
					<xs:appinfo>
						<units>s</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="tau_energy_98" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Energy confinement time estimated from the IPB98(y,2) scaling</xs:documentation>
					<xs:appinfo>
						<units>s</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="resistance" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Plasma electric resistance</xs:documentation>
					<xs:appinfo>
						<units>ohm</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="q_95" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>q at the 95% poloidal flux surface</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="power_ohm" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Ohmic power</xs:documentation>
					<xs:appinfo>
						<units>W</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="power_steady" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Total power coupled to the plasma minus dW/dt (correcting from transient energy content)</xs:documentation>
					<xs:appinfo>
						<units>W</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="power_radiated" type="summary_dynamic_flt_1d_1">
				<xs:annotation>
					<xs:documentation>Radiated power</xs:documentation>
					<xs:appinfo>
						<units>W</units>
