					<xs:documentation>Pedestal poloidal beta for electrons using the low field side magnetic poloidal field</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="beta_pol_pedestal_electron_hfs" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Pedestal poloidal beta for electrons using the high field side magnetic poloidal field</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="nustar_pedestal_electron" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Pedestal normalised collisionality for electrons</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="rhostar_pedestal_electron_lfs" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Pedestal normalised Larmor radius for electrons using the low field side magnetic field (important for spherical tokamaks)</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="rhostar_pedestal_electron_hfs" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Pedestal normalised Larmor radius for electrons using the high field side magnetic field (important for spherical tokamaks)</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="rhostar_pedestal_electron_magnetic_axis" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Pedestal normalised Larmor radius for electrons using the magnetic field on the magnetic axis (definition used in most tokamak litterature)</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="b_field_pol_average" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Poloidal field calculated at the position of the pedestal top (as determined by the fit) and averaged over the flux surface</xs:documentation>
					<xs:appinfo>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="b_field_pol_hfs" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Poloidal field calculated at the position of the pedestal top (as determined by the fit) on the high field side</xs:documentation>
					<xs:appinfo>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="b_field_pol_lfs" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Poloidal field calculated at the position of the pedestal top (as determined by the fit) on the low field side</xs:documentation>
					<xs:appinfo>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="b_field_hfs" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Total magnetic field calculated at the position of the pedestal top (as determined by the fit) on the high field side</xs:documentation>
					<xs:appinfo>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="b_field_lfs" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Total magnetic field calculated at the position of the pedestal top (as determined by the fit) on the low field side</xs:documentation>
					<xs:appinfo>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="b_field_tor_hfs" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Toroidal field calculated at the position of the pedestal top (as determined by the fit) on the high field side</xs:documentation>
					<xs:appinfo>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="b_field_tor_lfs" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Toroidal field calculated at the position of the pedestal top (as determined by the fit) on the low field side</xs:documentation>
					<xs:appinfo>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="coulomb_factor" type="summary_dynamic_flt_1d_2">
				<xs:annotation>
					<xs:documentation>Coulomb factor log(lambda)</xs:documentation>
					<xs:appinfo>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="parameters">
				<xs:annotation>
					<xs:documentation>Parameters of the fit</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>mixed</units>
						<coordinate1>1...5</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
