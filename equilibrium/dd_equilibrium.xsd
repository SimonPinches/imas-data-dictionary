<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="../Utilities/DD_Support.xsd"/>
	<xs:complexType name="equilibrium_convergence">
		<xs:annotation>
			<xs:documentation>Convergence details for the equilibrium calculation</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="iterations_n">
				<xs:annotation>
					<xs:documentation>Number of iterations carried out in the convergence loop</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_boundary">
		<xs:annotation>
			<xs:documentation>Geometry of the plasma boundary</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="type">
				<xs:annotation>
					<xs:documentation>0 (limiter) or 1 (diverted)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="outline" type="rz1d_dynamic_aos">
				<xs:annotation>
					<xs:documentation>RZ outline of the plasma boundary</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lcfs" type="rz1d_dynamic_aos">
				<xs:annotation>
					<xs:documentation>RZ description of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<lifecycle_status>obsolescent</lifecycle_status>
						<lifecycle_version>3.5.0</lifecycle_version>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="b_flux_pol_norm">
				<xs:annotation>
					<xs:documentation>Value of the normalised poloidal flux at which the boundary is taken</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="geometric_axis" type="rz0d_dynamic_aos">
				<xs:annotation>
					<xs:documentation>RZ position of the geometric axis (defined as (Rmin+Rmax) / 2 and (Zmin+Zmax) / 2 of the boundary)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="minor_radius">
				<xs:annotation>
					<xs:documentation>Minor radius of the plasma boundary (defined as (Rmax-Rmin) / 2 of the boundary)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="elongation">
				<xs:annotation>
					<xs:documentation>Elongation of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="elongation_upper">
				<xs:annotation>
					<xs:documentation>Elongation (upper half w.r.t. geometric axis) of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="elongation_lower">
				<xs:annotation>
					<xs:documentation>Elongation (lower half w.r.t. geometric axis) of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="triangularity">
				<xs:annotation>
					<xs:documentation>Triangularity of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<units>-</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="triangularity_upper">
				<xs:annotation>
					<xs:documentation>Upper triangularity of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<units>-</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="triangularity_lower">
				<xs:annotation>
					<xs:documentation>Lower triangularity of the plasma boundary</xs:documentation>
					<xs:appinfo>
						<units>-</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="x_point" type="rz0d_dynamic_aos" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Array of X-points, for each of them the RZ position is given</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="strike_point" type="rz0d_dynamic_aos" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Array of strike points, for each of them the RZ position is given</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="active_limiter_point" type="rz0d_dynamic_aos">
				<xs:annotation>
					<xs:documentation>RZ position of the active limiter point (point of the plasma boundary in contact with the limiter)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_global_quantities_magnetic_axis">
		<xs:annotation>
			<xs:documentation>R, Z, and Btor at magnetic axis, dynamic within a type 3 array of structure (index on time)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="r">
				<xs:annotation>
					<xs:documentation>Major radius of the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="z">
				<xs:annotation>
					<xs:documentation>Height of the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="b_tor">
				<xs:annotation>
					<xs:documentation>Total toroidal magnetic field at the magnetic axis</xs:documentation>
					<xs:appinfo>
						<lifecycle_status>obsolescent</lifecycle_status>
						<lifecycle_version>3.5.0</lifecycle_version>
						<type>dynamic</type>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="b_field_tor">
				<xs:annotation>
					<xs:documentation>Total toroidal magnetic field at the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_global_quantities_qmin">
		<xs:annotation>
			<xs:documentation>Position and value of q_min</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="value">
				<xs:annotation>
					<xs:documentation>Minimum q value</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rho_tor_norm">
				<xs:annotation>
					<xs:documentation>Minimum q position in normalised toroidal flux coordinate</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equlibrium_global_quantities">
		<xs:annotation>
			<xs:documentation>0D parameters of the equilibrium</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="beta_pol">
				<xs:annotation>
					<xs:documentation>Poloidal beta. Defined as betap = 4 int(p dV) / [R_0 * mu_0 * Ip^2]</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="beta_tor">
				<xs:annotation>
					<xs:documentation>Toroidal beta, defined as the volume-averaged total perpendicular pressure divided by (B0^2/(2*mu0)), i.e. beta_toroidal = 2 mu0 int(p dV) / V / B0^2</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="beta_normal">
				<xs:annotation>
					<xs:documentation>Normalised toroidal beta, defined as 100 * beta_tor * a[m] * B0 [T] / ip [MA] </xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="ip">
				<xs:annotation>
					<xs:documentation>Plasma current. Positive sign means anti-clockwise when viewed from above.</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="li_3">
				<xs:annotation>
					<xs:documentation>Internal inductance</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="volume">
				<xs:annotation>
					<xs:documentation>Total plasma volume</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>m^3</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="area">
				<xs:annotation>
					<xs:documentation>Area of the LCFS poloidal cross section</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>m^2</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="surface">
				<xs:annotation>
					<xs:documentation>Surface area of the toroidal flux surface</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>m^2</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="length_pol">
				<xs:annotation>
					<xs:documentation>Poloidal length of the magnetic surface</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="psi_axis">
				<xs:annotation>
					<xs:documentation>Poloidal flux at the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="psi_boundary">
				<xs:annotation>
					<xs:documentation>Poloidal flux at the selected plasma boundary </xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="magnetic_axis" type="equilibrium_global_quantities_magnetic_axis">
				<xs:annotation>
					<xs:documentation>Magnetic axis position and toroidal field</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="q_axis">
				<xs:annotation>
					<xs:documentation>q at the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="q_95">
				<xs:annotation>
					<xs:documentation>q at the 95% poloidal flux surface</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="q_min" type="equilibrium_global_quantities_qmin">
				<xs:annotation>
					<xs:documentation>Minimum q value and position</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="energy_mhd">
				<xs:annotation>
					<xs:documentation>Plasma energy content = 3/2 * int(p,dV) with p being the total pressure (thermal + fast particles) [J]. Time-dependent; Scalar</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>J</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="w_mhd">
				<xs:annotation>
					<xs:documentation>Plasma energy content = 3/2 * int(p,dV) with p being the total pressure (thermal + fast particles) [J]. Time-dependent; Scalar</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>J</units>
						<lifecycle_status>obsolescent</lifecycle_status>
						<lifecycle_version>3.14.0</lifecycle_version>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_constraints_pure_position">
		<xs:annotation>
			<xs:documentation>R,Z position constraint</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="position_measured" type="rz0d_dynamic_aos">
				<xs:annotation>
					<xs:documentation>Measured or estimated position</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="source">
				<xs:annotation>
					<xs:documentation>Path to the source data for this measurement in the IMAS data dictionary</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="time_measurement">
				<xs:annotation>
					<xs:documentation>Exact time slice used from the time array of the measurement source data. If the time slice does not exist in the time array of the source data, it means linear interpolation has been used</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>s</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="exact">
				<xs:annotation>
					<xs:documentation>Integer flag : 1 means exact data, taken as an exact input without being fitted; 0 means the equilibrium code does a least square fit</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="weight">
				<xs:annotation>
					<xs:documentation>Weight given to the measurement</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="standard_deviation">
				<xs:annotation>
					<xs:documentation>Standard deviation of the measurement</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="position_reconstructed" type="rz0d_dynamic_aos">
				<xs:annotation>
					<xs:documentation>Position estimated from the reconstructed equilibrium</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="chi_squared">
				<xs:annotation>
					<xs:documentation>Chi-squared difference of (reconstructed-measured)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_constraints_0D_position">
		<xs:annotation>
			<xs:documentation>Scalar constraint with R,Z,phi position</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="measured">
				<xs:annotation>
					<xs:documentation>Measured value</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="position" type="rzphi0d_dynamic_aos3">
				<xs:annotation>
					<xs:documentation>Position at which this measurement is given</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="source">
				<xs:annotation>
					<xs:documentation>Path to the source data for this measurement in the IMAS data dictionary</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="time_measurement">
				<xs:annotation>
					<xs:documentation>Exact time slice used from the time array of the measurement source data. If the time slice does not exist in the time array of the source data, it means linear interpolation has been used</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>s</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="exact">
				<xs:annotation>
					<xs:documentation>Integer flag : 1 means exact data, taken as an exact input without being fitted; 0 means the equilibrium code does a least square fit</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="weight">
				<xs:annotation>
					<xs:documentation>Weight given to the measurement</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="standard_deviation">
				<xs:annotation>
					<xs:documentation>Standard deviation of the measurement</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="reconstructed">
				<xs:annotation>
					<xs:documentation>Value calculated from the reconstructed equilibrium</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="chi_squared">
				<xs:annotation>
					<xs:documentation>Chi-squared difference of (reconstructed-measured)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_constraints_0D">
		<xs:annotation>
			<xs:documentation>Scalar constraint</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="measured">
				<xs:annotation>
					<xs:documentation>Measured value</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="source">
				<xs:annotation>
					<xs:documentation>Path to the source data for this measurement in the IMAS data dictionary</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="time_measurement">
				<xs:annotation>
					<xs:documentation>Exact time slice used from the time array of the measurement source data. If the time slice does not exist in the time array of the source data, it means linear interpolation has been used</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>s</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="exact">
				<xs:annotation>
					<xs:documentation>Integer flag : 1 means exact data, taken as an exact input without being fitted; 0 means the equilibrium code does a least square fit</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="weight">
				<xs:annotation>
					<xs:documentation>Weight given to the measurement</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="standard_deviation">
				<xs:annotation>
					<xs:documentation>Standard deviation of the measurement</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="reconstructed">
				<xs:annotation>
					<xs:documentation>Value calculated from the reconstructed equilibrium</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="chi_squared">
				<xs:annotation>
					<xs:documentation>Chi-squared difference of (reconstructed-measured)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_constraints_1D">
		<xs:annotation>
			<xs:documentation>Vector constraint (from a 1D array of measurements)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="measured">
				<xs:annotation>
					<xs:documentation>Measured values</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="source">
				<xs:annotation>
					<xs:documentation>Path to the source data for this measurement in the IMAS data dictionary</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="time_measurement">
				<xs:annotation>
					<xs:documentation>Exact time slices used from the time array of the measurement source data. If the time slice does not exist in the time array of the source data, it means linear interpolation has been used</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>s</units>
						<coordinate1>../measured</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="exact">
				<xs:annotation>
					<xs:documentation>Integer flag : 1 means exact data, taken as an exact input without being fitted; 0 means the equilibrium code does a least square fit</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../measured</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="weight">
				<xs:annotation>
					<xs:documentation>Weight given to each measured value</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>-</units>
						<coordinate1>../measured</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="standard_deviation">
				<xs:annotation>
					<xs:documentation>Standard deviation of the measurement</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
						<coordinate1>../measured</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="reconstructed">
				<xs:annotation>
					<xs:documentation>Value calculated from the reconstructed equilibrium</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
						<coordinate1>../measured</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="chi_squared">
				<xs:annotation>
					<xs:documentation>Chi-squared difference of (reconstructed-measured)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>as_parent</units>
						<coordinate1>../measured</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_constraints_magnetisation">
		<xs:annotation>
			<xs:documentation>Magnetisation constraints along R and Z axis</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="magnetisation_r" type="equilibrium_constraints_0D">
				<xs:annotation>
					<xs:documentation>Magnetisation M of the iron core segment along the major radius axis, assumed to be constant inside a given iron segment. Reminder : H = 1/mu0 * B - mur * M;</xs:documentation>
					<xs:appinfo>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="magnetisation_z" type="equilibrium_constraints_0D">
				<xs:annotation>
					<xs:documentation>Magnetisation M of the iron core segment along the vertical axis, assumed to be constant inside a given iron segment. Reminder : H = 1/mu0 * B - mur * M;</xs:documentation>
					<xs:appinfo>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_constraints">
		<xs:annotation>
			<xs:documentation>Measurements to constrain the equilibrium, output values and accuracy of the fit</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="b_field_tor_vacuum_r" type="equilibrium_constraints_0D">
				<xs:annotation>
					<xs:documentation>Vacuum field times major radius in the toroidal field magnet. Positive sign means anti-clockwise when viewed from above</xs:documentation>
					<xs:appinfo>
						<units>T.m</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="bpol_probe" type="equilibrium_constraints_0D" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of poloidal field probes</xs:documentation>
					<xs:appinfo>
						<coordinate1>IDS:magnetics/bpol_probe</coordinate1>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="diamagnetic_flux" type="equilibrium_constraints_0D">
				<xs:annotation>
					<xs:documentation>Diamagnetic flux</xs:documentation>
					<xs:appinfo>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="faraday_angle" type="equilibrium_constraints_0D" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of faraday angles</xs:documentation>
					<xs:appinfo>
						<coordinate1>IDS:polarimeter/channel</coordinate1>
						<units>rad</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="mse_polarisation_angle" type="equilibrium_constraints_0D" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of MSE polarisation angles</xs:documentation>
					<xs:appinfo>
						<coordinate1>IDS:mse/channel</coordinate1>
						<units>rad</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="flux_loop" type="equilibrium_constraints_0D" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of flux loops</xs:documentation>
					<xs:appinfo>
						<coordinate1>IDS:magnetics/flux_loop</coordinate1>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ip" type="equilibrium_constraints_0D">
				<xs:annotation>
					<xs:documentation>Plasma current. Positive sign means anti-clockwise when viewed from above</xs:documentation>
					<xs:appinfo>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="iron_core_segment" type="equilibrium_constraints_magnetisation" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Magnetisation M of a set of iron core segments</xs:documentation>
					<xs:appinfo>
						<coordinate1>IDS:iron_core/segment</coordinate1>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="n_e" type="equilibrium_constraints_0D" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of local density measurements</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>m^-3</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="n_e_line" type="equilibrium_constraints_0D" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of line integrated density measurements</xs:documentation>
					<xs:appinfo>
						<coordinate1>IDS:interferometer/channel</coordinate1>
						<units>m^-2</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pf_current" type="equilibrium_constraints_0D" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current in a set of poloidal field coils</xs:documentation>
					<xs:appinfo>
						<coordinate1>IDS:pf_active/coil</coordinate1>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pressure" type="equilibrium_constraints_0D" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of total pressure estimates</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>Pa</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="q" type="equilibrium_constraints_0D_position" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of safety factor estimates at various positions</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>Pa</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="x_point" type="equilibrium_constraints_pure_position" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Array of X-points, for each of them the RZ position is given</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_profiles_1d">
		<xs:annotation>
			<xs:documentation>Equilibrium profiles (1D radial grid) as a function of the poloidal flux</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="psi">
				<xs:annotation>
					<xs:documentation>Poloidal flux</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>1...N</coordinate1>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="phi">
				<xs:annotation>
					<xs:documentation>Toroidal flux</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="pressure">
				<xs:annotation>
					<xs:documentation>Pressure</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>Pa</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="f">
				<xs:annotation>
					<xs:documentation>Diamagnetic function (F=R B_Phi)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>T.m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="dpressure_dpsi">
				<xs:annotation>
					<xs:documentation>Derivative of pressure w.r.t. psi</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>Pa.Wb^-1</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="f_df_dpsi">
				<xs:annotation>
					<xs:documentation>Derivative of F w.r.t. Psi, multiplied with F</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>T^2.m^2/Wb</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="j_tor">
				<xs:annotation>
					<xs:documentation>Flux surface averaged toroidal current density = average(j_tor/R) / average(1/R)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>A/m^2</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="j_parallel">
				<xs:annotation>
					<xs:documentation>Flux surface averaged parallel current density = average(j.B) / B0, where B0 = Equilibrium/Global/Toroidal_Field/B0</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>A/m^2</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="q">
				<xs:annotation>
					<xs:documentation>Safety factor</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="magnetic_shear">
				<xs:annotation>
					<xs:documentation>Magnetic shear, defined as rho_tor/q . dq/drho_tor</xs:documentation>
					<xs:appinfo>
						<coordinate1>../psi</coordinate1>
						<units>-</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="r_inboard">
				<xs:annotation>
					<xs:documentation>Radial coordinate (major radius) on the inboard side of the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="r_outboard">
				<xs:annotation>
					<xs:documentation>Radial coordinate (major radius) on the outboard side of the magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rho_tor">
				<xs:annotation>
					<xs:documentation>Toroidal flux coordinate. The toroidal field used in its definition is indicated under vacuum_toroidal_field/b0</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rho_tor_norm">
				<xs:annotation>
					<xs:documentation>Normalised toroidal flux coordinate. The normalizing value for rho_tor_norm, is the toroidal flux coordinate at the equilibrium boundary (LCFS or 99.x % of the LCFS in case of a fixed boundary equilibium calculation) </xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="dpsi_drho_tor">
				<xs:annotation>
					<xs:documentation>Derivative of Psi with respect to Rho_Tor</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>Wb/m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="elongation">
				<xs:annotation>
					<xs:documentation>Elongation</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="triangularity_upper">
				<xs:annotation>
					<xs:documentation>Upper triangularity w.r.t. magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="triangularity_lower">
				<xs:annotation>
					<xs:documentation>Lower triangularity w.r.t. magnetic axis</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../psi</coordinate1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="volume">
				<xs:annotation>
					<xs:documentation>Volume enclosed in the flux surface</xs:documentation>
