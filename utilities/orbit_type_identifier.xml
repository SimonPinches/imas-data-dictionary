<constants name="orbit_type" identifier="yes" create_mapping_function="yes">
<header>
Translation table for orbit_type_identifier_definitions.
</header>

<ddInstance xpath="complexType:ids_distributions_d_ggd_orbit_type/identifier"/>

<int name="unspecified"    description="unspecified"                                                                   >0</int>
<int name="none"           description="No orbit"                                                                      >1</int>
<int name="lost"           description="Orbit on lost trajectory"                                                      >2</int>
<int name="lost_counter"   description="Lost orbit for co-passing particle; closed orbit for counter-passing particle" >3</int>
<int name="counter"        description="Counter-passing only trajectory"                                               >4</int>
<int name="co"             description="Co-passing only trajectory"                                                    >5</int>
<int name="co_counter"     description="Either co- or counter-passing trajectory"                                      >6</int>
<int name="stagnation"     description="Co-passing trajectory on the low-field side only"                              >7</int>
<int name="banana"         description="Trapped trajectory with both midplane passages on the low-field side"          >8</int>
