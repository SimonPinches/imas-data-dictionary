	<xs:complexType name="B_Tor_Vacuum_1">
		<xs:annotation>
			<xs:documentation>Characteristics of the vacuum toroidal field. Timebase assumed to be one level above</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="R0">
				<xs:annotation>
					<xs:documentation>Reference major radius where the vacuum toroidal magnetic field is given (usually a fixed position such as the middle of the vessel at the equatorial midplane)</xs:documentation>
					<xs:appinfo>
						<type>CONSTANT</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="B0">
				<xs:annotation>
					<xs:documentation>Vacuum toroidal field at R0 [T]; Positive sign means anti-clockwise when viewed from above</xs:documentation>
					<xs:appinfo>
						<type>SIGNAL</type>
						<units>T</units>
						<axis1>../../Timebase</axis1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="B_Tor_Vacuum_2">
		<xs:annotation>
			<xs:documentation>Characteristics of the vacuum toroidal field. Timebase assumed to be two levels above</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="R0">
				<xs:annotation>
					<xs:documentation>Reference major radius where the vacuum toroidal magnetic field is given (usually a fixed position such as the middle of the vessel at the equatorial midplane)</xs:documentation>
