		<xs:complexType>
			<xs:sequence>
				<xs:element name="Nuclei" type="Plasma_Composition_Nuclei" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Array of nuclei considered.</xs:documentation>
						<xs:appinfo>
							<axis1>1...N_Nuclei</axis1>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="Ions" type="Plasma_Composition_Ions" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Array of main plasma ions.</xs:documentation>
						<xs:appinfo>
							<axis1>1...N_Ions</axis1>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
