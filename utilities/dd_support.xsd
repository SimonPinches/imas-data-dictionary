<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:complexType name="plasma_composition_charge_states">
		<xs:annotation>
			<xs:documentation>Array of charge states considered</xs:documentation>
			<xs:appinfo>
				<coordinate1>1...N</coordinate1>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="z_min">
				<xs:annotation>
					<xs:documentation>Minimum Z of the charge state bundle</xs:documentation>
					<xs:appinfo>
						<units>Elementary Charge Unit</units>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="z_max">
				<xs:annotation>
					<xs:documentation>Maximum Z of the charge state bundle</xs:documentation>
					<xs:appinfo>
						<units>Elementary Charge Unit</units>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="label">
				<xs:annotation>
					<xs:documentation>String identifying charge state (e.g. C+, C+2 , C+3, C+4, C+5, C+6, ...)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="plasma_composition_ions">
		<xs:annotation>
			<xs:documentation>Array of plasma ions.</xs:documentation>
			<xs:appinfo>
				<coordinate1>1...N</coordinate1>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="a">
				<xs:annotation>
					<xs:documentation>Mass of atom</xs:documentation>
					<xs:appinfo>
						<units>Atomic Mass Unit</units>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="z_ion">
				<xs:annotation>
					<xs:documentation>Ion charge (of the dominant ionisation state; lumped ions are allowed)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>Elementary Charge Unit</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="z_n">
				<xs:annotation>
					<xs:documentation>Nuclear charge</xs:documentation>
					<xs:appinfo>
						<units>Elementary Charge Unit</units>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="label">
				<xs:annotation>
					<xs:documentation>String identifying ion (e.g. H+, D+, T+, He+2, C+, ...)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="multiple_charge_state_flag">
				<xs:annotation>
					<xs:documentation>Multiple charge state calculation flag : 0-Only one charge state is considered; 1-Multiple charge state are considered and are described in the Charge_States structure</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="charge_states" type="plasma_composition_charge_states" maxOccurs="10">
				<xs:annotation>
					<xs:documentation>Array of charge states considered for this ion (non-empty if Multiple_Charge_State_Flag = 1)</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="plasma_composition_species">
		<xs:annotation>
			<xs:documentation>Description of simple species (elements) without declaration of their ionisation state</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="a">
				<xs:annotation>
					<xs:documentation>Mass of atom</xs:documentation>
					<xs:appinfo>
						<units>Atomic Mass Unit</units>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="z_n">
				<xs:annotation>
					<xs:documentation>Nuclear charge</xs:documentation>
					<xs:appinfo>
						<units>Elementary Charge Unit</units>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="label">
				<xs:annotation>
					<xs:documentation>String identifying the species (e.g. H, D, T, ...)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="plasma_composition_neutrals_components">
		<xs:annotation>
			<xs:documentation>Components to the atom or molecule</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="a">
				<xs:annotation>
					<xs:documentation>Mass of atom</xs:documentation>
					<xs:appinfo>
						<units>Atomic Mass Unit</units>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="z_n">
				<xs:annotation>
					<xs:documentation>Nuclear charge</xs:documentation>
					<xs:appinfo>
						<units>Elementary Charge Unit</units>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="multiplicity">
				<xs:annotation>
					<xs:documentation>Multiplicity of the atom</xs:documentation>
					<xs:appinfo>
						<units>Elementary Charge Unit</units>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="plasma_composition_neutrals">
		<xs:annotation>
			<xs:documentation>Array of neutral atoms or molecules</xs:documentation>
			<xs:appinfo>
				<coordinate1>1...N</coordinate1>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="components" type="plasma_composition_neutrals_components" maxOccurs="10">
				<xs:annotation>
					<xs:documentation>Array of components to the atom or molecule</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="energies">
				<xs:annotation>
					<xs:documentation>Types energies considered for this neutral species : 0=cold, 1=thermal, 2= fast, 3=NBI</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="label">
				<xs:annotation>
					<xs:documentation>String identifying the atom or molecule (e.g. D2, DT, CD4, ...)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="plasma_composition">
		<xs:annotation>
			<xs:documentation>Generic declaration of Plasma Composition for a simulation</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ions" type="plasma_composition_ions" maxOccurs="5">
					<xs:annotation>
						<xs:documentation>Array of plasma ions</xs:documentation>
						<xs:appinfo>
							<coordinate1>1...N</coordinate1>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="neutrals" type="plasma_composition_neutrals" maxOccurs="5">
					<xs:annotation>
						<xs:documentation>Array of neutrals</xs:documentation>
						<xs:appinfo>
							<coordinate1>1...N</coordinate1>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="code">
		<xs:annotation>
			<xs:documentation>Generic decription of the code specific parameters for the code that has produced this IDS</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="name">
					<xs:annotation>
						<xs:documentation>Name of the code</xs:documentation>
						<xs:appinfo>
							<type>constant</type>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:group ref="STR_0D"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="version">
					<xs:annotation>
						<xs:documentation>Version of the code</xs:documentation>
						<xs:appinfo>
							<type>constant</type>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:group ref="STR_0D"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="parameters">
					<xs:annotation>
						<xs:documentation>List of the code specific parameters in XML format</xs:documentation>
						<xs:appinfo>
							<type>constant</type>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:group ref="STR_0D"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="output_flag">
					<xs:annotation>
						<xs:documentation>Output flag : 0 means the run is successful, other values mean some difficulty has been encountered, the exact meaning is then code specific. Negative values mean the result shall not be used.</xs:documentation>
						<xs:appinfo>
							<coordinate1>../../time</coordinate1>
							<type>dynamic</type>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:group ref="INT_1D"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="parameters_input">
		<xs:annotation>
			<xs:documentation>Code parameters block passed from the wrapper to the subroutine. Does not appear as such in the data structure. This is inserted in DD_Support.xsd for automatic declaration in the Fortran and C++ type definitions.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="parameters_value" type="str_type">
					<xs:annotation>
						<xs:documentation>Actual value of the code parameters (instance of Code_Parameters/Parameters in XML format)</xs:documentation>
						<xs:appinfo>
							<type>constant</type>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="parameters_default" type="str_type">
					<xs:annotation>
						<xs:documentation>Default value of the code parameters (instance of Code_Parameters/Parameters in XML format)</xs:documentation>
						<xs:appinfo>
							<type>constant</type>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="schema" type="str_type">
					<xs:annotation>
						<xs:documentation>Code parameters schema</xs:documentation>
						<xs:appinfo>
							<type>constant</type>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="schedule_waveform">
		<xs:annotation>
			<xs:documentation>Description of a named waveform, normally for scheduling</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name">
				<xs:annotation>
					<xs:documentation>Reference name</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="value">
				<xs:annotation>
					<xs:documentation>Values of the reference for the timebase</xs:documentation>
					<xs:appinfo>
						<coordinate1>../time</coordinate1>
						<units>mixed</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="time"/>
						<xs:group ref="FLT_1D"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="errorbar">
		<xs:annotation>
			<xs:documentation>Structure for 1D dynamic errorbar</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="type">
				<xs:annotation>
					<xs:documentation>Type of errorbar description which is used. Refers to the name of the sibling fields.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="dynamic">
				<xs:annotation>
					<xs:documentation>Absolute error provided as a dynamic of same dimension as the related field.</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../../time</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="constant">
				<xs:annotation>
					<xs:documentation>Relative error in % provided as a constant</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="expression">
				<xs:annotation>
					<xs:documentation>Absolute error provided as an Expression.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="b_tor_vacuum_1">
		<xs:annotation>
			<xs:documentation>Characteristics of the vacuum toroidal field. time assumed to be one level above</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="r0">
				<xs:annotation>
					<xs:documentation>Reference major radius where the vacuum toroidal magnetic field is given (usually a fixed position such as the middle of the vessel at the equatorial midplane)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="b0">
				<xs:annotation>
					<xs:documentation>Vacuum toroidal field at R0 [T]; When using cocos = 11 (expected for ITER data), positive sign means anti-clockwise when viewed from above. The product R0B0 must be consistent with the b_tor_vacuum_r field of the tf IDS.</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>T</units>
						<coordinate1>../../time</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="b_tor_vacuum_2">
		<xs:annotation>
			<xs:documentation>Characteristics of the vacuum toroidal field. time assumed to be two levels above</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="r0">
				<xs:annotation>
					<xs:documentation>Reference major radius where the vacuum toroidal magnetic field is given (usually a fixed position such as the middle of the vessel at the equatorial midplane)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="b0">
				<xs:annotation>
					<xs:documentation>Vacuum toroidal field at b0. When using cocos = 11 (expected for ITER data), positive sign means anti-clockwise when viewed from above. The product r0*b0 must be consistent with the b_tor_vacuum_r field of the tf IDS.</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>T</units>
						<coordinate1>../../../time</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="core_radial_grid">
		<xs:annotation>
			<xs:documentation>1D radial grid for core* IDSs</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="rho_tor_norm">
				<xs:annotation>
					<xs:documentation>Normalised toroidal flux coordinate. The normalizing value for rho_tor_norm, is the toroidal flux coordinate at the equilibrium boundary (LCFS or 99.x % of the LCFS in case of a fixed boundary equilibium calculation) </xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>1...N</coordinate1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rho_tor">
				<xs:annotation>
					<xs:documentation>Toroidal flux coordinate. The toroidal field used in its definition is indicated under vacuum_toroidal_field/b0</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../rho_tor_norm</coordinate1>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="psi">
				<xs:annotation>
					<xs:documentation>Poloidal magnetic flux</xs:documentation>
					<xs:appinfo>
						<coordinate1>../rho_tor_norm</coordinate1>
						<units>Wb</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="volume">
				<xs:annotation>
					<xs:documentation>Volume enclosed inside the magnetic surface</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../rho_tor_norm</coordinate1>
						<units>m^3</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="area">
				<xs:annotation>
					<xs:documentation>Cross-sectional area of the flux surface</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../rho_tor_norm</coordinate1>
						<units>m^2</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_profiles_2d_grid">
		<xs:annotation>
			<xs:documentation>Definition of the 2D grid</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dim1">
				<xs:annotation>
					<xs:documentation>First dimension values</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>1...N</coordinate1>
						<units>mixed</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="dim2">
				<xs:annotation>
					<xs:documentation>Second dimension values</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>1...N</coordinate1>
						<units>mixed</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equilibrium_coordinate_system">
		<xs:annotation>
			<xs:documentation>Flux surface coordinate system on a square grid of flux and poloidal angle</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="grid_type" type="identifier">
				<xs:annotation>
					<xs:documentation>Type of coordinate system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grid" type="equilibrium_profiles_2d_grid">
				<xs:annotation>
					<xs:documentation>Definition of the 2D grid</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="r">
				<xs:annotation>
					<xs:documentation>Values of the major radius on the grid</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../grid/dim1</coordinate1>
						<coordinate2>../grid/dim2</coordinate2>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="z">
				<xs:annotation>
					<xs:documentation>Values of the Height on the grid</xs:documentation>
					<xs:appinfo>
