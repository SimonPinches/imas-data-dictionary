					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="inconsistent_curvature_drift">
				<xs:annotation>
					<xs:documentation>Flag = 1 if the beta_prime contribution to the curvature drift is neglected, 0 otherwise. Only allowed in combination with the neglect of parallel magnetic field fluctuations</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_pitch_only">
				<xs:annotation>
					<xs:documentation>Flag = 1 if only pitch-angle scattering is retained, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_momentum_conservation">
				<xs:annotation>
					<xs:documentation>Flag = 1 if the collision operator conserves momentum, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_energy_conservation">
				<xs:annotation>
					<xs:documentation>Flag = 1 if the collision operator conserves energy, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_finite_larmor_radius">
				<xs:annotation>
					<xs:documentation>Flag = 1 if finite larmor radius effects are retained in the collision operator, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="initial_value_run">
				<xs:annotation>
					<xs:documentation>Flag = 1 if this is an initial value run, 0 for an eigenvalue run</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="non_linear_run">
				<xs:annotation>
					<xs:documentation>Flag = 1 if this is a non-linear run, 0 for a linear run</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="time_interval_norm">
				<xs:annotation>
					<xs:documentation>Normalised time interval used to average fluxes and moments from non-linear runs</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
