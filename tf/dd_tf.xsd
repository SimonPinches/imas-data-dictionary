<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="../utilities/dd_support.xsd"/>
	<xs:complexType name="tf_coil_conductor_elements">
		<xs:annotation>
			<xs:documentation>Elements descibring the conductor contour</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="names">
				<xs:annotation>
					<xs:documentation>Name or description of every element</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="types">
				<xs:annotation>
					<xs:documentation>Type of every element: 1: line segment, its ends are given by the start and end points; index = 2: arc of a circle</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<coordinate1>../names</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="start_points" type="rzphi1d_static">
				<xs:annotation>
					<xs:documentation>Position of the start point of every element</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="end_points" type="rzphi1d_static">
				<xs:annotation>
					<xs:documentation>Position of the end point of every element</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="centres" type="rzphi1d_static">
				<xs:annotation>
					<xs:documentation>Position of the centre of the arc of a circle of every element (meaningful only if type/index = 2, fill with default/empty value otherwise)</xs:documentation>
				</xs:annotation>
			</xs:element>
