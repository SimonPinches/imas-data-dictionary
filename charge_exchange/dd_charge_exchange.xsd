	<xs:complexType name="charge_exchange_channel_processed_line">
		<xs:annotation>
			<xs:documentation>Description of a processed line</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label">
				<xs:annotation>
