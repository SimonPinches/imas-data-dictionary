					<xs:documentation>Flag describing how the profile has been calculated : 0-not calculated 1-interpretative; 2-calculated by the transport solver; 3-calculated by a separate code; 4-used value from the previous time step</xs:documentation>
					<xs:appinfo>
						<axis1>../Timebase</axis1>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Boundary_Condition" type="Core_Profiles_Boundary_2D_Ne">
				<xs:annotation>
					<xs:documentation>Boundary condition for the transport equation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Source_Term" type="Core_Profiles_Source_Term_2D_Ne">
				<xs:annotation>
					<xs:documentation>Total source term used in the transport equation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Transport_Coefficients" type="Core_Profiles_Transport_Coefficients_2D_NeTe">
				<xs:annotation>
					<xs:documentation>Total transport coefficients used in the transport equation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Fluxes" type="Core_Profiles_Fluxes_2D_Ne">
				<xs:annotation>
					<xs:documentation>Fluxes of the transported quantity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Timebase"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Core_Profiles_2D_Psi">
		<xs:annotation>
			<xs:documentation>Structure for a main 2D field of core transport equations, Psi</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Value">
				<xs:annotation>
					<xs:documentation>Field value</xs:documentation>
					<xs:appinfo>
						<axis1>../../Rho_Tor_Norm</axis1>
						<axis2>../Timebase</axis2>
						<units>Wb</units>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="dValue_dRho_Tor">
				<xs:annotation>
					<xs:documentation>Radial derivative (dValue/dRho_Tor)</xs:documentation>
					<xs:appinfo>
						<axis1>../../Rho_Tor_Norm</axis1>
						<axis2>../Timebase</axis2>
						<units>Wb.m^-1</units>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="d2Value_dRho_Tor2">
				<xs:annotation>
					<xs:documentation>Second order radial derivative</xs:documentation>
					<xs:appinfo>
						<axis1>../../Rho_Tor_Norm</axis1>
						<axis2>../Timebase</axis2>
						<units>Wb.m^-2</units>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="dValue_dT_CRho_Tor_Norm">
				<xs:annotation>
					<xs:documentation>Time derivative of the poloidal flux at constant rho_tor_norm [V]</xs:documentation>
					<xs:appinfo>
						<axis1>../../Rho_Tor_Norm</axis1>
						<axis2>../Timebase</axis2>
						<units>V</units>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="dValue_dT_CPhi">
				<xs:annotation>
					<xs:documentation>Time derivative of the poloidal flux at constant toroidal flux</xs:documentation>
					<xs:appinfo>
						<axis1>../../Rho_Tor_Norm</axis1>
						<axis2>../Timebase</axis2>
						<units>V</units>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Source">
				<xs:annotation>
					<xs:documentation>Source of the profile (any comment describing the origin of the profile : code, path to diagnostic signals, massaging, ...)</xs:documentation>
					<xs:appinfo>
						<type>CONSTANT</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Calculation_Mode">
				<xs:annotation>
					<xs:documentation>Flag describing how the profile has been calculated : 0-not calculated 1-interpretative; 2-calculated by the transport solver; 3-calculated by a separate code; 4-used value from the previous time step</xs:documentation>
					<xs:appinfo>
						<axis1>../Timebase</axis1>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Boundary_Condition" type="Core_Profiles_Boundary_2D_Psi">
				<xs:annotation>
					<xs:documentation>Boundary condition for the transport equation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="J_Non_Inductive">
				<xs:annotation>
					<xs:documentation>Non-inductive parallel current density = average(jni.B) / B0, where B0 = Core_Profiles/Toroidal_Field/ B0</xs:documentation>
					<xs:appinfo>
						<axis1>../../Rho_Tor_Norm</axis1>
						<axis2>../Timebase</axis2>
						<units>A/m^2</units>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Conductivity_Parallel">
				<xs:annotation>
					<xs:documentation>Parallel conductivity</xs:documentation>
					<xs:appinfo>
						<axis1>../../Rho_Tor_Norm</axis1>
						<axis2>../Timebase</axis2>
						<units>ohm^-1.m^-1</units>
						<type>SIGNAL</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element ref="Timebase"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Core_Profiles">
		<xs:annotation>
			<xs:documentation>Core plasma 1D profiles as a function of the normalised toroidal flux coordinate. TBD: toroidal velocity</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="IDS_Properties"/>
				<xs:element name="Rho_Tor_Norm">
					<xs:annotation>
						<xs:documentation>Normalised toroidal flux coordinate values (= rho_tor normalised to the value at the last radial grid point, which is usually almost at the Last Closed Flux Surface)</xs:documentation>
						<xs:appinfo>
							<type>SIGNAL</type>
							<axis1>1...N_Rho</axis1>
							<axis2>../Timebase</axis2>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:group ref="FLT_2D"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="Rho_Tor">
					<xs:annotation>
						<xs:documentation>Toroidal flux coordinate (not normalised)</xs:documentation>
						<xs:appinfo>
							<type>SIGNAL</type>
							<axis1>../Rho_Tor_Norm</axis1>
							<axis2>../Timebase</axis2>
							<units>m</units>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:group ref="FLT_2D"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="dRho_Tor_dT">
					<xs:annotation>
						<xs:documentation>Time derivative of Rho_Tor</xs:documentation>
						<xs:appinfo>
							<type>SIGNAL</type>
							<axis1>../Rho_Tor_Norm</axis1>
							<axis2>../Timebase</axis2>
							<units>m/s</units>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:group ref="FLT_2D"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="Toroidal_Field" type="Core_Profiles_Toroidal_Field">
					<xs:annotation>
						<xs:documentation>Toroidal field information entering the definition of Rho_Tor, for reference only</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Plasma_Composition"/>
				<xs:element name="Psi" type="Core_Profiles_2D_Psi">
					<xs:annotation>
						<xs:documentation>Poloidal magnetic flux</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Te" type="Core_Profiles_2D_Te">
					<xs:annotation>
						<xs:documentation>Electron temperature</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Ti" type="Core_Profiles_3D_Ti">
					<xs:annotation>
						<xs:documentation>Ion temperature</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Ne" type="Core_Profiles_2D_Ne">
					<xs:annotation>
						<xs:documentation>Electron density</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Ni" type="Core_Profiles_3D_Ni">
					<xs:annotation>
						<xs:documentation>Ion density</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Derived_Profiles" type="Core_Profiles_Derived_Profiles">
					<xs:annotation>
						<xs:documentation>Profiles derived from the fields solved in the transport equations, or from experiment.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Derived_Global" type="Core_Profiles_Derived_Global">
					<xs:annotation>
						<xs:documentation>Various global quantities calculated from the fields solved in the transport equations and from the Derived Profiles</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Code_Parameters"/>
				<xs:element ref="Timebase"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
