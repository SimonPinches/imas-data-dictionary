			<xs:element name="Ni_Transport" type="Core_Transport_Models_Density_3">
				<xs:annotation>
					<xs:documentation>Transport coefficients for ion density equation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Ti_Transport" type="Core_Transport_Models_Energy_TorMom_3">
				<xs:annotation>
					<xs:documentation>Transport coefficients for ion energy equation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Multiple_Charge_State_Flag">
				<xs:annotation>
					<xs:documentation>Multiple charge state calculation flag : 0-Only one charge state is considered; 1-Multiple charge state are considered and are described in the Charge_States structure</xs:documentation>
					<xs:appinfo>
						<type>CONSTANT</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Charge_States" type="Core_Transport_Models_Ions_Charge_States" maxOccurs="10">
				<xs:annotation>
					<xs:documentation>Transport coefficients related to the different charge states of the ion species</xs:documentation>
					<xs:appinfo>
						<axis1>1...N_Charge_States</axis1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
